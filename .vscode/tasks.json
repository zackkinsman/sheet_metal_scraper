{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Open with Qt Designer",
            "type": "shell",
            "command": "${workspaceFolder}/tenderapp/Scripts/pyside6-designer.exe",
            "args": [
                "${file}"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": []
        }
    ]
}